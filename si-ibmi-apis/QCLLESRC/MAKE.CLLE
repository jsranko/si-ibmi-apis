 PGMENDE:
/* ---------------------------------------------------------------*/
             CLRLIB     LIB(QTEMP)
/* ---------------------------------------------------------------*/
/*   RMVLIBLE   LIB(&libobj)                                      */
/* ============================================================== */
             SUBR       crtSRVPGM
                RTVMBRD    FILE(&libsrc/QRPGLESRC) +
                           MBR(&object) +
                           SRCTYPE(&srctype)
                IF         COND(&srctype *EQ 'RPGLE') THEN(DO)
                   CRTRPGMOD  MODULE(QTEMP/&object) +
                              SRCFILE(&libsrc/QRPGLESRC) +
                              SRCMBR(&object) +
                              DBGVIEW(*ALL) +
                              DBGENCKEY(&key) +
                              OPTION(*EVENTF) +
                              TGTRLS(&tgtrls)
                   MONMSG     MSGID(CPF0000 ICE0000) EXEC(DO)
                      DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                      RTNSUBR    RTNVAL(1)
                   ENDDO
                ENDDO
                ELSE       DO
                   CRTSQLRPGI OBJ(QTEMP/&object) +
                              SRCFILE(&libsrc/QRPGLESRC) +
                              SRCMBR(&object) +
                              OBJTYPE(*MODULE) +
                              REPLACE(*YES) +
                              OPTION(*XREF *EVENTF) +
                              DBGVIEW(*SOURCE) +
                              DBGENCKEY(&key) +
                              RPGPPOPT(*LVL2) +
                              TGTRLS(&tgtrls)
                   MONMSG     MSGID(CPF0000 ICE0000) EXEC(DO)
                      DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                      RTNSUBR    RTNVAL(1)
                   ENDDO
                ENDDO

                CRTSRVPGM  SRVPGM(&libobj/&object) +
                           MODULE(QTEMP/&object) +
                           EXPORT(*SRCFILE) +
                           SRCFILE(&libsrc/QSRVSRC) +
                           SRCMBR(*SRVPGM) +
                           OPTION(*DUPPROC) +
                           TGTRLS(&tgtrls)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPJOBLOG
                   RTNSUBR    RTNVAL(1)
                ENDDO
             ENDSUBR
/* ============================================================== */
             SUBR       addBNDDIRE
                RMVBNDDIRE BNDDIR(&libobj/SIIIA) OBJ((&object *SRVPGM))
                MONMSG     MSGID(CPD5D1C)
                ADDBNDDIRE BNDDIR(&libobj/SIIIA) OBJ((&object *SRVPGM *DEFER))
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPJOBLOG
                   RTNSUBR    RTNVAL(1)
                ENDDO
             ENDSUBR
/* ============================================================== */
             SUBR       crtBNDRPG
                CRTBNDRPG  PGM(&libobj/&object) +
                           SRCFILE(&libsrc/QRPGLESRC) +
                           SRCMBR(&object) +
                           OPTION(*EVENTF) +
                           DBGVIEW(*ALL) +
                           DBGENCKEY(&key) +
                           TGTRLS(&tgtrls)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                   RTNSUBR    RTNVAL(1)
                ENDDO

             ENDSUBR
/* ============================================================== */
             SUBR       crtSQLRPGI
                CRTSQLRPGI OBJ(&libobj/&object) +
                           SRCFILE(&libsrc/QRPGLESRC) +
                           SRCMBR(&object) +
                           OPTION(*EVENTF) +
                           RPGPPOPT(*LVL2) +
                           DBGVIEW(*SOURCE) +
                           DBGENCKEY(&key) +
                           TGTRLS(&tgtrls)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                   RTNSUBR    RTNVAL(1)
                ENDDO

             ENDSUBR
/* ============================================================== */
             SUBR       crtDSPF
                CRTDSPF    FILE(&libobj/&object) +
                            SRCFILE(&libsrc/QDDSSRC) +
                            SRCMBR(&object)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                   RTNSUBR    RTNVAL(1)
                ENDDO

             ENDSUBR
/* ============================================================== */
             SUBR       crtPF
                CRTPF      FILE(&libobj/&object) +
                           SRCFILE(&libsrc/QDDSSRC) +
                           SRCMBR(&object) +
                           IGCDTA(*N)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                   RTNSUBR    RTNVAL(1)
                ENDDO

             ENDSUBR
/* ============================================================== */
             SUBR       runSQLS
                RUNSQLSTM  SRCFILE(&libsrc/QDDLSRC) +
                           SRCMBR(&object) +
                           COMMIT(*NONE) +
                           MARGINS(240) +
                           OUTPUT(*PRINT)
                MONMSG     MSGID(CPF0000) EXEC(DO)
                   DSPSPLF    FILE(&object) JOB(*) SPLNBR(*LAST)
                   RTNSUBR    RTNVAL(1)
                ENDDO

             ENDSUBR

/* ============================================================== */
             SUBR       runINIT
                ADDBNDDIRE BNDDIR(&libobj/SIIIA) OBJ((QSHELL/QZSHAPI))
                MONMSG     MSGID(CPF0000)
             ENDSUBR
/* ============================================================== */
             SUBR       crtMSGF
                DLTMSGF    MSGF(SIMSGF)
                MONMSG     MSGID(CPF2105)
                CRTMSGF    MSGF(&libobj/SIMSGF) TEXT('SI: Messages for Framework SIIIA')
                MONMSG     MSGID(CPF0000)

                /* Messages for SIXCP  */
                QSYS/ADDMSGD MSGID(SIE0000) +
                             MSGF(&libobj/SIMSGF) +
                             MSG('&1') +
                             SECLVL('Generic Message. No more details.') +
                             FMT((*CHAR 50))

                QSYS/ADDMSGD MSGID(SIE0001) +
                             MSGF(&libobj/SIMSGF) +
                             MSG('Error, cannot change current directory.') +
                             SECLVL('See Joblog.')

                QSYS/ADDMSGD MSGID(SIE0002) +
                             MSGF(&libobj/SIMSGF) +
                             MSG('Error, cannot get current directory.') +
                             SECLVL('See Joblog.')

                /* Messages for SICURL */
                QSYS/ADDMSGD MSGID(SIEAA00) +
                             MSGF(&libobj/SIMSGF) +
                             MSG('Command &1 not supported by SICURL') +
                             SECLVL('For more details see Prozedure SICURL_runCmd -
                             in SRVPGM:SICURL.') FMT((*CHAR 6))

             ENDSUBR
/* ============================================================== */

             ENDPGM
